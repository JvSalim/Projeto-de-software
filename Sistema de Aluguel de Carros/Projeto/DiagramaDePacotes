@startuml

package "Model" {
    class Usuario {}
    class Cliente extends Usuario {}
    class Agente extends Usuario {}
    class Empresa extends Agente {}
    class Banco extends Agente {}
    class Automovel {}
    class Pedido {}
    class Contrato {}
    class ContratoCredito {}
    class Empregador {}
}

package "Controller" {
    class PedidoController {}
    class ContratoController {}
    class UsuarioController {}
}

package "View" {}

Pedido "1" o-- "1" Automovel 
Pedido "0..1" o-- "1" Contrato 
Contrato "0..1" o-- "1" ContratoCredito 
Pedido "0..1" -- "1" Agente 
Cliente "1" o-- "0..*" Empregador 
Automovel "1" -- "0..*" Contrato 
PedidoController .up.> Pedido 
ContratoController ..> Contrato 
UsuarioController ..> Usuario 
PedidoController ..> View  
ContratoController ..> View 
UsuarioController ..> View

@enduml
